{
    "version": 2,
    "name": "guesser-backend",
    "builds": [
      {
        "src": "nest build",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/main.ts",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "functions": {
      "preBuild": {
        "command": "npm run build"
      }
    }
  }
  